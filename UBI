   "cell_type": "code",
   "execution_count": 233,
   "id": "19d0cec4",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "RMSE (Modèle Ridge): 1925.2619877285629\n"
     ]
